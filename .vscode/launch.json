{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch App",
        "type": "node",
        "request": "launch",
        "skipFiles": [
          "**\\node_modules\\**",
          "<node_internals>/**",
          "${workspaceFolder}/<node_internals>/**",
          "${workspaceFolder}\\<node_internals>\\**",
          "${workspaceFolder}/<node_internals>/**/*.js",
          "${workspaceFolder}\\<node_internals>\\**\\*.js",
          "${workspaceFolder}/node_modules/**",
          "${workspaceFolder}\\node_modules\\**",
          "${workspaceFolder}/node_modules/**/*.js",
          "${workspaceFolder}\\node_modules\\**\\*.js"
        ],
        "program": "${workspaceFolder}/dist/apps/d2-vendor-alert/app.js"
      }
    ]  
  }