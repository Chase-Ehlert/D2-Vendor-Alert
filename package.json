{
  "name": "d2-vendor-alert",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint \"./src/**/*.ts\" --fix",
    "tsc": "tsc",
    "tests": "jest",
    "tests-with-coverage": "jest --coverage --collectCoverageFrom='src/**/*.ts'",
    "start-alert": "npm run tsc && node dist/apps/d2-vendor-alert/app.js",
    "start-notifier": "npm run tsc && node dist/apps/discord-notifier/app.js",
    "jsdoc": "jsdoc -c jsdoc.json"
  },
  "type": "module",
  "author": "Chase Ehlert",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.3.4",
    "discord.js": "^14.11.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "mongodb": "^5.0.0",
    "mongoose": "^6.9.0",
    "mustache-express": "^1.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.2",
    "@types/mustache-express": "^1.2.2",
    "@types/node": "^18.16.18",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "cpy": "^10.1.0",
    "eslint": "^8.43.0",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^4.3.1",
    "jest": "^29.5.0",
    "jsdoc": "^4.0.2",
    "mustache": "^4.2.0",
    "pino": "^8.14.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.3",
    "typescript": "5.1.6"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(t|j)sx?$": ["ts-jest", {"useESM": true}]
    },
    "extensionsToTreatAsEsm": [".ts"],
    "modulePathIgnorePatterns": [
      ".dist/",
      ".src/testing-helpers/",
      ".src/apps/d2-vendor-alert/app.ts",
      ".src/apps/d2-vendor-alert/@types",
      ".src/apps/discord-notifier/app.ts",
      ".src/presentation/discord/deploy-commands-script.ts",
      ".src/presentation/discord/deploy-commands.ts"
    ],
    "moduleFileExtensions": ["ts", "js", "json", "node"]
  }
}
