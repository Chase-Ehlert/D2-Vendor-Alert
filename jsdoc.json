{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["src"],
        "includePattern": ".+\\.ts(doc|x)?$",
        "excludePattern": "(node_modules/|docs)"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false
    },
    "opts": {
        "destination": "./jsdoc",
        "recurse": true
    }
}
